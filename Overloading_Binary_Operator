#include<iostream>

using namespace std;
class Amlan {

			int obj1;
		public:

			void getobj1(){
				cin>>obj1;
			}

			void operator !=(Amlan object2){
				if (obj1 != object2.obj1)
				cout<<"\nObject 1 is != to Object 2"<<endl;
			else
			cout<<"\nObject_1 is == Object 2"<<endl;
				}
};


int main(){
	Amlan object1,object2;
	cout<<"Enter the First Number:"<<endl;
	object1.getobj1();
	cout<<"Enter the Second Number"<<endl;
	object2.getobj1();
	object1!=object2;
    return 0;
}
